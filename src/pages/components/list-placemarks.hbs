{{#each placemarks}}
    <div class="columns mt-6">
        <div class="box box-link-hover-shadow ">
            <h2 class="title">
                {{name}}
                <i class="fas fa-map-pin" style="color:rgb(196,14,21)"></i>
            </h2>
            <p>{{description}}</p>
            <p class="has-text-weight-bold" >Lat. {{latitude}} Lgt. {{longitude}}</p>
        </div>
    </div>
{{/each}}